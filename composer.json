{
  "name": "wp-forge/user-last-modified",
  "description": "A WordPress code library for tracking the user last modified date and time.",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Micah Wood",
      "email": "micah@wpscholar.com"
    }
  ],
  "autoload": {
    "classmap": [
      "src/UserLastModified.php"
    ],
    "files": [
